    <item>
      <title></title>
      <link>http://localhost:1313/qr-codes/dotted-qr-generator/</link>
      <pubDate>Mon, 01 Jan 0001 00:00:00 +0000</pubDate>
      <guid>http://localhost:1313/qr-codes/dotted-qr-generator/</guid>
      <description>&lt;!DOCTYPE html&gt;&#xD;&#xA;&lt;html lang=&#34;hi&#34;&gt;&#xD;&#xA;&lt;head&gt;&#xD;&#xA;  &lt;meta charset=&#34;UTF-8&#34; /&gt;&#xD;&#xA;  &lt;title&gt;Styled QR Generator&lt;/title&gt;&#xD;&#xA;&#xD;&#xA;  &lt;!-- QR Code Styling library --&gt;&#xD;&#xA;  &lt;script type=&#34;text/javascript&#34;&#xD;&#xA;    src=&#34;https://unpkg.com/qr-code-styling@1.5.0/lib/qr-code-styling.js&#34;&gt;&#xD;&#xA;  &lt;/script&gt;&#xD;&#xA;&#xD;&#xA;  &lt;style&gt;&#xD;&#xA;    * { box-sizing: border-box; font-family: &#34;Poppins&#34;, system-ui, sans-serif; }&#xD;&#xA;    body {&#xD;&#xA;      min-height: 100vh;&#xD;&#xA;      margin: 0;&#xD;&#xA;      display: flex;&#xD;&#xA;      justify-content: center;&#xD;&#xA;      align-items: center;&#xD;&#xA;      background: #0f172a;&#xD;&#xA;      color: #e5e7eb;&#xD;&#xA;    }&#xD;&#xA;    .qr-wrapper {&#xD;&#xA;      background: #020617;&#xD;&#xA;      border-radius: 18px;&#xD;&#xA;      padding: 24px 22px 26px;&#xD;&#xA;      max-width: 420px;&#xD;&#xA;      width: 100%;&#xD;&#xA;      box-shadow: 0 18px 35px rgba(0,0,0,0.45);&#xD;&#xA;      border: 1px solid rgba(148, 163, 184, 0.2);&#xD;&#xA;    }&#xD;&#xA;    .qr-header {&#xD;&#xA;      font-weight: 700;&#xD;&#xA;      font-size: 20px;&#xD;&#xA;      margin-bottom: 4px;&#xD;&#xA;    }&#xD;&#xA;    .qr-sub {&#xD;&#xA;      font-size: 13px;&#xD;&#xA;      color: #9ca3af;&#xD;&#xA;      margin-bottom: 16px;&#xD;&#xA;    }&#xD;&#xA;    label {&#xD;&#xA;      font-size: 13px;&#xD;&#xA;      display: block;&#xD;&#xA;      margin-bottom: 4px;&#xD;&#xA;      color: #cbd5f5;&#xD;&#xA;    }&#xD;&#xA;    input[type=&#34;text&#34;] {&#xD;&#xA;      width: 100%;&#xD;&#xA;      padding: 10px 12px;&#xD;&#xA;      border-radius: 10px;&#xD;&#xA;      border: 1px solid #1e293b;&#xD;&#xA;      background: #020617;&#xD;&#xA;      color: #e5e7eb;&#xD;&#xA;      font-size: 14px;&#xD;&#xA;      outline: none;&#xD;&#xA;    }&#xD;&#xA;    input[type=&#34;text&#34;]:focus {&#xD;&#xA;      border-color: #2563eb;&#xD;&#xA;      box-shadow: 0 0 0 1px rgba(37, 99, 235, 0.4);&#xD;&#xA;    }&#xD;&#xA;    .controls {&#xD;&#xA;      display: flex;&#xD;&#xA;      gap: 10px;&#xD;&#xA;      margin-top: 10px;&#xD;&#xA;      align-items: center;&#xD;&#xA;    }&#xD;&#xA;    select, input[type=&#34;color&#34;] {&#xD;&#xA;      padding: 5px 8px;&#xD;&#xA;      border-radius: 8px;&#xD;&#xA;      border: 1px solid #1f2933;&#xD;&#xA;      background: #020617;&#xD;&#xA;      color: #e5e7eb;&#xD;&#xA;      font-size: 12px;&#xD;&#xA;    }&#xD;&#xA;    .btn {&#xD;&#xA;      border: none;&#xD;&#xA;      cursor: pointer;&#xD;&#xA;      border-radius: 999px;&#xD;&#xA;      padding: 7px 16px;&#xD;&#xA;      font-size: 13px;&#xD;&#xA;      font-weight: 600;&#xD;&#xA;      display: inline-flex;&#xD;&#xA;      align-items: center;&#xD;&#xA;      gap: 6px;&#xD;&#xA;      transition: transform 0.08s ease, box-shadow 0.08s ease, background 0.12s ease;&#xD;&#xA;      white-space: nowrap;&#xD;&#xA;    }&#xD;&#xA;    .btn-generate {&#xD;&#xA;      background: linear-gradient(135deg, #1d4ed8, #38bdf8);&#xD;&#xA;      color: white;&#xD;&#xA;      box-shadow: 0 10px 20px rgba(37,99,235,0.4);&#xD;&#xA;    }&#xD;&#xA;    .btn-generate:hover { transform: translateY(-1px); }&#xD;&#xA;    .btn-download {&#xD;&#xA;      background: #020617;&#xD;&#xA;      color: #e5e7eb;&#xD;&#xA;      border: 1px solid #1f2937;&#xD;&#xA;    }&#xD;&#xA;    .btn-download:hover { transform: translateY(-1px); }&#xD;&#xA;    .qr-output {&#xD;&#xA;      margin-top: 18px;&#xD;&#xA;      padding: 10px;&#xD;&#xA;      border-radius: 14px;&#xD;&#xA;      background: radial-gradient(circle at top, #0b1120 0, #020617 55%, #000 100%);&#xD;&#xA;      display: flex;&#xD;&#xA;      justify-content: center;&#xD;&#xA;      align-items: center;&#xD;&#xA;    }&#xD;&#xA;    #qr-canvas {&#xD;&#xA;      background: white;&#xD;&#xA;      border-radius: 12px;&#xD;&#xA;      padding: 8px;&#xD;&#xA;    }&#xD;&#xA;    .note {&#xD;&#xA;      margin-top: 8px;&#xD;&#xA;      font-size: 11px;&#xD;&#xA;      color: #9ca3af;&#xD;&#xA;      text-align: center;&#xD;&#xA;    }&#xD;&#xA;  &lt;/style&gt;&#xD;&#xA;&lt;/head&gt;&#xD;&#xA;&lt;body&gt;&#xD;&#xA;  &lt;div class=&#34;qr-wrapper&#34;&gt;&#xD;&#xA;    &lt;div class=&#34;qr-header&#34;&gt;Styled QR Generator&lt;/div&gt;&#xD;&#xA;    &lt;div class=&#34;qr-sub&#34;&gt;Blue dotted QR (same style जैसा aapne photo me diya hai).&lt;/div&gt;&#xD;&#xA;&#xD;&#xA;    &lt;label for=&#34;qr-text&#34;&gt;Text / URL&lt;/label&gt;&#xD;&#xA;    &lt;input id=&#34;qr-text&#34; type=&#34;text&#34;&#xD;&#xA;           placeholder=&#34;https://example.com ya koi text likhiye&#34; /&gt;&#xD;&#xA;&#xD;&#xA;    &lt;div class=&#34;controls&#34;&gt;&#xD;&#xA;      &lt;button class=&#34;btn btn-generate&#34; id=&#34;btn-generate&#34;&gt;&#xD;&#xA;        ⚡ Generate&#xD;&#xA;      &lt;/button&gt;&#xD;&#xA;&#xD;&#xA;      &lt;select id=&#34;qr-size&#34; title=&#34;Size&#34;&gt;&#xD;&#xA;        &lt;option value=&#34;200&#34;&gt;200 px&lt;/option&gt;&#xD;&#xA;        &lt;option value=&#34;260&#34; selected&gt;260 px&lt;/option&gt;&#xD;&#xA;        &lt;option value=&#34;320&#34;&gt;320 px&lt;/option&gt;&#xD;&#xA;      &lt;/select&gt;&#xD;&#xA;&#xD;&#xA;      &lt;input type=&#34;color&#34; id=&#34;qr-color&#34; value=&#34;#003a77&#34; title=&#34;Dot color&#34; /&gt;&#xD;&#xA;      &lt;button class=&#34;btn btn-download&#34; id=&#34;btn-download&#34;&gt;&#xD;&#xA;        ⬇ Download PNG&#xD;&#xA;      &lt;/button&gt;&#xD;&#xA;    &lt;/div&gt;&#xD;&#xA;&#xD;&#xA;    &lt;div class=&#34;qr-output&#34;&gt;&#xD;&#xA;      &lt;div id=&#34;qr-canvas&#34;&gt;&lt;/div&gt;&#xD;&#xA;    &lt;/div&gt;&#xD;&#xA;    &lt;div class=&#34;note&#34;&gt;&#xD;&#xA;      Default color: #003a77 (aap blue tone change kar sakte hain).&#xD;&#xA;    &lt;/div&gt;&#xD;&#xA;  &lt;/div&gt;&#xD;&#xA;&#xD;&#xA;  &lt;script&gt;&#xD;&#xA;    // Default QR instance&#xD;&#xA;    let qrCode = new QRCodeStyling({&#xD;&#xA;      width: 260,&#xD;&#xA;      height: 260,&#xD;&#xA;      type: &#34;svg&#34;,&#xD;&#xA;      data: &#34;https://example.com&#34;,&#xD;&#xA;      margin: 0,&#xD;&#xA;      dotsOptions: {&#xD;&#xA;        color: &#34;#003a77&#34;,   // blue dots&#xD;&#xA;        type: &#34;dots&#34;        // round dots like your sample&#xD;&#xA;      },&#xD;&#xA;      cornersSquareOptions: {&#xD;&#xA;        color: &#34;#003a77&#34;,&#xD;&#xA;        type: &#34;extra-rounded&#34;  // गोल outer eye&#xD;&#xA;      },&#xD;&#xA;      cornersDotOptions: {&#xD;&#xA;        color: &#34;#003a77&#34;&#xD;&#xA;      },&#xD;&#xA;      backgroundOptions: {&#xD;&#xA;        color: &#34;#ffffff&#34;    // सफेद background&#xD;&#xA;      }&#xD;&#xA;    });&#xD;&#xA;&#xD;&#xA;    const qrCanvas = document.getElementById(&#34;qr-canvas&#34;);&#xD;&#xA;    const inputText = document.getElementById(&#34;qr-text&#34;);&#xD;&#xA;    const inputSize = document.getElementById(&#34;qr-size&#34;);&#xD;&#xA;    const inputColor = document.getElementById(&#34;qr-color&#34;);&#xD;&#xA;    const btnGenerate = document.getElementById(&#34;btn-generate&#34;);&#xD;&#xA;    const btnDownload = document.getElementById(&#34;btn-download&#34;);&#xD;&#xA;&#xD;&#xA;    qrCode.append(qrCanvas);&#xD;&#xA;&#xD;&#xA;    // Generate / Update QR&#xD;&#xA;    function updateQR() {&#xD;&#xA;      const data = inputText.value.trim() || &#34;https://example.com&#34;;&#xD;&#xA;      const size = parseInt(inputSize.value, 10) || 260;&#xD;&#xA;      const color = inputColor.value || &#34;#003a77&#34;;&#xD;&#xA;&#xD;&#xA;      qrCode.update({&#xD;&#xA;        data,&#xD;&#xA;        width: size,&#xD;&#xA;        height: size,&#xD;&#xA;        dotsOptions: { color, type: &#34;dots&#34; },&#xD;&#xA;        cornersSquareOptions: { color, type: &#34;extra-rounded&#34; },&#xD;&#xA;        cornersDotOptions: { color }&#xD;&#xA;      });&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    btnGenerate.addEventListener(&#34;click&#34;, updateQR);&#xD;&#xA;&#xD;&#xA;    // Download PNG&#xD;&#xA;    btnDownload.addEventListener(&#34;click&#34;, () =&gt; {&#xD;&#xA;      qrCode.download({&#xD;&#xA;        name: &#34;styled-qr&#34;,&#xD;&#xA;        extension: &#34;png&#34;&#xD;&#xA;      });&#xD;&#xA;    });&#xD;&#xA;&#xD;&#xA;    // First render&#xD;&#xA;    updateQR();&#xD;&#xA;  &lt;/script&gt;&#xD;&#xA;&lt;/body&gt;&#xD;&#xA;&lt;/html&gt;</description>
    </item>
